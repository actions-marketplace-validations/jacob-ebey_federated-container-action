{
  "version": 2,
  "builds": [
    {
      "src": "/test/dist/vercel.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["/test/.container/server/**"]
      }
    },
    {
      "src": "/test/.container/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/test/.container/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/test/dist/vercel.js"
    }
  ]
}
